{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/roi-calculator-block",
  "version": "0.1.0",
  "title": "ROI Calculator Block",
  "category": "widgets",
  "icon": "calculator",
  "description": "ROI Calculator Block.",
  "example": {},
  "supports": {
    "html": false
  },
  "attributes": {
    "percentageIncrease": {
      "type": "number",
      "default": 1
    },
    "hours": {
      "type": "number",
      "default": 12
    },
    "days": {
      "type": "number",
      "default": 5
    },
    "weeksPerYear": {
      "type": "number",
      "default": 52
    },
    "unitsPerHour": {
      "type": "number",
      "default": 22500
    },
    "profitPerUnit": {
      "type": "number",
      "default": 2
    }
  },
  "textdomain": "roi-calculator-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}